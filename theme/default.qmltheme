@import url(system:Ambiance/qmltheme/default.qmltheme);
@qml-import(theme 0.1, ..);

@qml-mapping(.thin-slider, , ThinSliderDelegate);
.thin-slider {
    backgroundImage: url(artwork/zoom_bar@18.png);
    thumbSpacing: 0;
    thumbImage: url(artwork/zoom_point@18.png);
    thumbWidth: units.gu(1.5);
    thumbHeight: units.gu(1.5);
}
